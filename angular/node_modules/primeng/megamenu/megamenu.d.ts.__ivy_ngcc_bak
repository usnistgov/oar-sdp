import { ElementRef, Renderer2 } from '@angular/core';
import { MegaMenuItem, MenuItem } from 'primeng/api';
export declare class MegaMenu {
    el: ElementRef;
    renderer: Renderer2;
    model: MegaMenuItem[];
    style: any;
    styleClass: string;
    orientation: string;
    autoZIndex: boolean;
    baseZIndex: number;
    activeItem: any;
    hideTimeout: any;
    constructor(el: ElementRef, renderer: Renderer2);
    onItemMouseEnter(event: any, item: any, menuitem: MegaMenuItem): void;
    onItemMouseLeave(event: any, link: any): void;
    itemClick(event: any, item: MenuItem | MegaMenuItem): void;
    getColumnClass(menuitem: MegaMenuItem): any;
}
export declare class MegaMenuModule {
}
